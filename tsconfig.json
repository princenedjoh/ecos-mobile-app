{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "typeRoots": ["./src/custom types/"]
  },
  "paths": {
    "@screens/*": ["./src/screens/*"],
    "@components/*": ["./src/components/*"],
    "@constants/*": ["./src/constants/*"],
    "@navigation/*": ["./src/navigation/*"],
    "@utils/*": ["./src/utils/*"],
    "@styles/*": ["./src/styles/*"],
    "@assets/*": ["./src/assets/*"],
    "@/*": ["./src/*"],
  }
}
